on init 
	
	declare $n := 0
	declare $i := 0
	declare @topLabel

end on


function SS_func_topDisplay()
	
	{3 groups change together (6 groups of 3) and sync}
	message($n)
	pgs_create_key(number, 5)

end function




on async_complete

end on




on note

	$n := $EVENT_NOTE
	call SS_func_topDisplay()

end on